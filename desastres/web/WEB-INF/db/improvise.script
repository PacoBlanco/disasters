CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE USUARIOS(ID_USUARIOS INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOMBRE_USUARIO VARCHAR(250),PASSWORD VARCHAR(250),TIPO_USUARIO VARCHAR(250))
CREATE MEMORY TABLE RESOURCES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),NOMBRE VARCHAR(50),DESCRIPCION VARCHAR(200),INFO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,DIRECCION VARCHAR(50),ESTADO VARCHAR(15),IDASSIGNED INTEGER)
CREATE MEMORY TABLE CATASTROFES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MARCADOR VARCHAR(15),TIPO VARCHAR(15),CANTIDAD INTEGER,NOMBRE VARCHAR(50),DESCRIPCION VARCHAR(200),INFO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,DIRECCION VARCHAR(50),ESTADO VARCHAR(15),SIZE VARCHAR(15),TRAFFIC VARCHAR(15),FECHA TIMESTAMP,IDASSIGNED INTEGER,MODIFICADO TIMESTAMP DEFAULT CURRENT_TIMESTAMP,USUARIO INTEGER,SINTOMAS VARCHAR(250))
CREATE MEMORY TABLE PROYECTOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PROYECTO VARCHAR(250))
CREATE MEMORY TABLE RELACIONES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_USUARIO INTEGER NOT NULL,ID_PROYECTO INTEGER NOT NULL,CONSTRAINT SYS_FK_56 FOREIGN KEY(ID_USUARIO) REFERENCES USUARIOS(ID_USUARIOS),CONSTRAINT SYS_FK_57 FOREIGN KEY(ID_PROYECTO) REFERENCES PROYECTOS(ID))
CREATE MEMORY TABLE MENSAJES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MENSAJE VARCHAR(250),NIVEL INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP)
ALTER TABLE USUARIOS ALTER COLUMN ID_USUARIOS RESTART WITH 8
ALTER TABLE RESOURCES ALTER COLUMN ID RESTART WITH 16
ALTER TABLE CATASTROFES ALTER COLUMN ID RESTART WITH 5
ALTER TABLE PROYECTOS ALTER COLUMN ID RESTART WITH 3
ALTER TABLE RELACIONES ALTER COLUMN ID RESTART WITH 9
ALTER TABLE MENSAJES ALTER COLUMN ID RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
