ALTER TABLE HISTORIAL ALTER COLUMN ID RESTART WITH 1
ALTER TABLE MENSAJES ALTER COLUMN ID RESTART WITH 1
ALTER TABLE ASOCIACIONES_HERIDOS_EMERGENCIAS ALTER COLUMN ID RESTART WITH 2
ALTER TABLE ACTIVIDADES ALTER COLUMN ID RESTART WITH 1
ALTER TABLE ASOCIACIONES_EMERGENCIAS_CON_TIPOS_USUARIOS ALTER COLUMN ID RESTART WITH 1
ALTER TABLE ASOCIACIONES_USUARIOS_CON_TIPOS_USUARIOS ALTER COLUMN ID RESTART WITH 1
ALTER TABLE ASOCIACIONES_ACTIVIDADES_CON_TIPOS_USUARIOS ALTER COLUMN ID RESTART WITH 1
ALTER TABLE SINTOMAS ALTER COLUMN ID RESTART WITH 1
