CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TIPOS_ESTADOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(500))
CREATE MEMORY TABLE TIPOS_EMERGENCIAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(500))
CREATE MEMORY TABLE TIPOS_HERIDOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(500))
CREATE MEMORY TABLE TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),NIVEL INTEGER,DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE TIPOS_ACTIVIDADES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),TIPO_EMERGENCIA VARCHAR(15),ESTADO_EMERGENCIA VARCHAR(15),DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE TIPOS_EVENTOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE TIPOS_SINTOMAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOMBRE_USUARIO VARCHAR(250),PASSWORD VARCHAR(250),TIPO_USUARIO VARCHAR(250),NOMBRE_REAL VARCHAR(250),CORREO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,LOCALIZACION BOOLEAN,PROYECTO VARCHAR(15))
CREATE MEMORY TABLE CATASTROFES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MARCADOR VARCHAR(15),TIPO VARCHAR(15),CANTIDAD INTEGER,NOMBRE VARCHAR(50),DESCRIPCION VARCHAR(200),INFO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,DIRECCION VARCHAR(50),ESTADO VARCHAR(15),SIZE VARCHAR(15),TRAFFIC VARCHAR(15),FECHA TIMESTAMP,MODIFICADO TIMESTAMP DEFAULT CURRENT_TIMESTAMP,USUARIO INTEGER,PLANTA INTEGER,IDASSIGNED INTEGER)
CREATE MEMORY TABLE HISTORIAL(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_USUARIO INTEGER,TIPO VARCHAR(15),ID_TIPO INTEGER,ID_EMERGENCIA INTEGER,EVENTO VARCHAR(250),FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_11 FOREIGN KEY(ID_USUARIO) REFERENCES USUARIOS(ID),CONSTRAINT SYS_FK_13 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID))
CREATE MEMORY TABLE MENSAJES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MENSAJE VARCHAR(250),NIVEL INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP)
CREATE MEMORY TABLE ASOCIACIONES_HERIDOS_EMERGENCIAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_HERIDO INTEGER,ID_EMERGENCIA INTEGER,ESTADO VARCHAR(15),FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_21 FOREIGN KEY(ID_HERIDO) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_22 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID))
CREATE MEMORY TABLE ACTIVIDADES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_USUARIO INTEGER,ID_EMERGENCIA INTEGER,ID_TIPO_ACTIVIDAD INTEGER,ESTADO VARCHAR(15),FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_31 FOREIGN KEY(ID_USUARIO) REFERENCES USUARIOS(ID),CONSTRAINT SYS_FK_32 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_33 FOREIGN KEY(ID_TIPO_ACTIVIDAD) REFERENCES TIPOS_ACTIVIDADES(ID))
CREATE MEMORY TABLE ASOCIACIONES_EMERGENCIAS_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_EMERGENCIA INTEGER,LOCALIZACION INTEGER,CONSTRAINT SYS_FK_41 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_42 FOREIGN KEY(ID_TIPO_EMERGENCIA) REFERENCES TIPOS_EMERGENCIAS(ID))
CREATE MEMORY TABLE ASOCIACIONES_USUARIOS_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_USUARIO_VER INTEGER,VISUALIZAR INTEGER,CONSTRAINT SYS_FK_51 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_52 FOREIGN KEY(ID_TIPO_USUARIO_VER) REFERENCES TIPOS_USUARIOS(ID))
CREATE MEMORY TABLE ASOCIACIONES_ACTIVIDADES_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_ACTIVIDAD INTEGER,CONSTRAINT SYS_FK_61 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_62 FOREIGN KEY(ID_TIPO_ACTIVIDAD) REFERENCES TIPOS_ACTIVIDADES(ID))
CREATE MEMORY TABLE SINTOMAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_HERIDO INTEGER,ID_SINTOMA INTEGER,ESTADO VARCHAR(15),FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_71 FOREIGN KEY(ID_HERIDO) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_72 FOREIGN KEY(ID_SINTOMA) REFERENCES TIPOS_SINTOMAS(ID))
