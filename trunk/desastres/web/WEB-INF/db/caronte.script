CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TIPOS_ESTADOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO_ESTADO VARCHAR(15),DESCRIPCION VARCHAR(500))
CREATE MEMORY TABLE TIPOS_MARCADORES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO_MARCADOR VARCHAR(15),DESCRIPCION VARCHAR(500))
CREATE MEMORY TABLE TIPOS_CATASTROFES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_MARCADOR INTEGER,TIPO_CATASTROFE VARCHAR(15),DESCRIPCION VARCHAR(500),CONSTRAINT SYS_FK_01 FOREIGN KEY(ID_MARCADOR) REFERENCES TIPOS_MARCADORES(ID))
CREATE MEMORY TABLE TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),ID_CATASTROFE INTEGER,NIVEL INTEGER,DESCRIPCION VARCHAR(250),CONSTRAINT SYS_FK_02 FOREIGN KEY(ID_CATASTROFE) REFERENCES TIPOS_CATASTROFES(ID))
CREATE MEMORY TABLE TIPOS_ACTIVIDADES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),TIPO_MARCADOR INTEGER,TIPO_EMERGENCIA INTEGER,ESTADO_EMERGENCIA INTEGER,DESCRIPCION VARCHAR(250),CONSTRAINT SYS_FK_03 FOREIGN KEY(TIPO_MARCADOR) REFERENCES TIPOS_MARCADORES(ID),CONSTRAINT SYS_FK_04 FOREIGN KEY(TIPO_EMERGENCIA) REFERENCES TIPOS_CATASTROFES(ID),CONSTRAINT SYS_FK_05 FOREIGN KEY(ESTADO_EMERGENCIA) REFERENCES TIPOS_ESTADOS(ID))
CREATE MEMORY TABLE TIPOS_EVENTOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE TIPOS_SINTOMAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,TIPO VARCHAR(15),DESCRIPCION VARCHAR(250))
CREATE MEMORY TABLE USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NOMBRE_USUARIO VARCHAR(250),PASSWORD VARCHAR(250),TIPO_USUARIO INTEGER,NOMBRE_REAL VARCHAR(250),CORREO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,LOCALIZACION BOOLEAN,PROYECTO VARCHAR(15),CONSTRAINT SYS_FK_11 FOREIGN KEY(TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID))
CREATE MEMORY TABLE CATASTROFES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,MARCADOR INTEGER,TIPO INTEGER,CANTIDAD INTEGER,NOMBRE VARCHAR(50),DESCRIPCION VARCHAR(250),INFO VARCHAR(250),LATITUD FLOAT,LONGITUD FLOAT,DIRECCION VARCHAR(250),SIZE VARCHAR(15),TRAFFIC VARCHAR(15),PLANTA INTEGER,ESTADO INTEGER,IDASSIGNED INTEGER,FECHA TIMESTAMP,MODIFICADO TIMESTAMP DEFAULT CURRENT_TIMESTAMP,USUARIO INTEGER,CONSTRAINT SYS_FK_21 FOREIGN KEY(MARCADOR) REFERENCES TIPOS_MARCADORES(ID),CONSTRAINT SYS_FK_22 FOREIGN KEY(TIPO) REFERENCES TIPOS_CATASTROFES(ID),CONSTRAINT SYS_FK_23 FOREIGN KEY(ESTADO) REFERENCES TIPOS_ESTADOS(ID),CONSTRAINT SYS_FK_24 FOREIGN KEY(USUARIO) REFERENCES USUARIOS(ID))
CREATE MEMORY TABLE HISTORIAL(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_USUARIO INTEGER,TIPO VARCHAR(15),ID_TIPO INTEGER,ID_EMERGENCIA INTEGER,EVENTO VARCHAR(250),FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_31 FOREIGN KEY(ID_USUARIO) REFERENCES USUARIOS(ID),CONSTRAINT SYS_FK_32 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID))
CREATE MEMORY TABLE MENSAJES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CREADOR INTEGER,MENSAJE VARCHAR(250),NIVEL INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_41 FOREIGN KEY(CREADOR) REFERENCES USUARIOS(ID))
CREATE MEMORY TABLE ASOCIACIONES_HERIDOS_EMERGENCIAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_HERIDO INTEGER,ID_EMERGENCIA INTEGER,ESTADO INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_51 FOREIGN KEY(ID_HERIDO) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_52 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_53 FOREIGN KEY(ESTADO) REFERENCES TIPOS_ESTADOS(ID))
CREATE MEMORY TABLE ACTIVIDADES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_USUARIO INTEGER,ID_EMERGENCIA INTEGER,ID_TIPO_ACTIVIDAD INTEGER,ESTADO INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_61 FOREIGN KEY(ID_USUARIO) REFERENCES USUARIOS(ID),CONSTRAINT SYS_FK_62 FOREIGN KEY(ID_EMERGENCIA) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_63 FOREIGN KEY(ID_TIPO_ACTIVIDAD) REFERENCES TIPOS_ACTIVIDADES(ID),CONSTRAINT SYS_FK_64 FOREIGN KEY(ESTADO) REFERENCES TIPOS_ESTADOS(ID))
CREATE MEMORY TABLE ASOCIACIONES_EMERGENCIAS_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_EMERGENCIA INTEGER,LOCALIZACION INTEGER,CONSTRAINT SYS_FK_71 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_72 FOREIGN KEY(ID_TIPO_EMERGENCIA) REFERENCES TIPOS_CATASTROFES(ID))
CREATE MEMORY TABLE ASOCIACIONES_USUARIOS_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_USUARIO_VER INTEGER,VISUALIZAR INTEGER,CONSTRAINT SYS_FK_81 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_82 FOREIGN KEY(ID_TIPO_USUARIO_VER) REFERENCES TIPOS_USUARIOS(ID))
CREATE MEMORY TABLE ASOCIACIONES_ACTIVIDADES_CON_TIPOS_USUARIOS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_TIPO_USUARIO INTEGER,ID_TIPO_ACTIVIDAD INTEGER,CONSTRAINT SYS_FK_91 FOREIGN KEY(ID_TIPO_USUARIO) REFERENCES TIPOS_USUARIOS(ID),CONSTRAINT SYS_FK_92 FOREIGN KEY(ID_TIPO_ACTIVIDAD) REFERENCES TIPOS_ACTIVIDADES(ID))
CREATE MEMORY TABLE SINTOMAS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ID_HERIDO INTEGER,ID_SINTOMA INTEGER,ESTADO INTEGER,FECHA TIMESTAMP DEFAULT CURRENT_TIMESTAMP,CONSTRAINT SYS_FK_54 FOREIGN KEY(ID_HERIDO) REFERENCES CATASTROFES(ID),CONSTRAINT SYS_FK_55 FOREIGN KEY(ID_SINTOMA) REFERENCES TIPOS_SINTOMAS(ID),CONSTRAINT SYS_FK_56 FOREIGN KEY(ESTADO) REFERENCES TIPOS_ESTADOS(ID))
ALTER TABLE TIPOS_ESTADOS ALTER COLUMN ID RESTART WITH 6
